// ========== Customer ==========

model Customer {
  id                   String                        @id
  type_id              Int?
  badge_id             Int?
  class_id             Int?
  cart                 Cart?
  badge                CustomerBadge?                @relation(fields: [badge_id], references: [id])
  class                CustomerClass?                @relation(fields: [class_id], references: [id])
  user                 User                          @relation(fields: [id], references: [id], onDelete: Cascade)
  type                 CustomerType?                 @relation(fields: [type_id], references: [id])
  addresses            CustomerAddress[]
  receiptConfirmations CustomerReceiptConfirmation[]
  stats                CustomerStat?

  @@index([badge_id], map: "customer_badge_id_fkey")
  @@index([class_id], map: "customer_class_id_fkey")
  @@index([type_id], map: "customer_type_id_fkey")
  @@map("customer")
}

model CustomerType {
  id          Int        @id @default(autoincrement())
  name        String     @unique
  description String?
  created_at  DateTime   @default(now())
  updated_at  DateTime   @updatedAt
  deleted_at  DateTime?
  customers   Customer[]

  @@map("customer_type")
}

model CustomerAddress {
  id               Int       @id @default(autoincrement())
  customer_id      String
  city             String?
  country          String?   @default("Egypt")
  postal_code      String?
  latitude         Decimal?  @db.Decimal(10, 8)
  longitude        Decimal?  @db.Decimal(11, 8)
  recipient_name   String?
  recipient_phone  String?
  created_at       DateTime  @default(now())
  updated_at       DateTime  @updatedAt
  deleted_at       DateTime?
  address          String?
  apartment_number String?
  area_id          String?
  building_number  String?
  floor_number     String?
  is_default       Boolean   @default(false)
  name             String
  notes            String?   @db.Text
  customer         Customer  @relation(fields: [customer_id], references: [id])
  orders           Order[]

  @@index([customer_id], map: "customer_address_customer_id_fkey")
  @@map("customer_address")
}

model CustomerStat {
  customer_id     String    @id
  last_order_date DateTime?
  total_spent     Decimal?
  total_orders    Int?
  created_at      DateTime  @default(now())
  updated_at      DateTime  @updatedAt
  customer        Customer  @relation(fields: [customer_id], references: [id])

  @@map("customer_stat")
}

model CustomerBadge {
  id             Int        @id @default(autoincrement())
  name           String     @unique
  description    String?    @db.Text
  created_at     DateTime   @default(now())
  updated_at     DateTime   @updatedAt
  description_ar String?    @db.Text
  max_amount     Float
  min_amount     Float
  name_ar        String     @unique
  ratio          Int
  ads            Ad[]
  customers      Customer[]
  offers         Offer[]

  @@map("customer_badge")
}

model CustomerClass {
  id          Int                @id @default(autoincrement())
  name        CustomerClassName?
  description String?            @db.Text
  created_at  DateTime           @default(now())
  updated_at  DateTime           @updatedAt
  ad          Ad[]
  customers   Customer[]
  offer       Offer[]

  @@map("customer_class")
}

model Cart {
  id          Int        @id @default(autoincrement())
  customer_id String     @unique
  total_price Decimal
  created_at  DateTime   @default(now())
  updated_at  DateTime   @updatedAt
  deleted_at  DateTime?
  customer    Customer   @relation(fields: [customer_id], references: [id], onDelete: Cascade)
  cart_item   CartItem[]
  orders      Order[]

  @@map("cart")
}

model CartItem {
  id           Int         @id @default(autoincrement())
  cart_id      Int
  product_id   String
  unit_id      Int
  quantity     Int         @default(1)
  created_at   DateTime    @default(now())
  updated_at   DateTime    @updatedAt
  offer_id     Int?
  cart         Cart        @relation(fields: [cart_id], references: [id], onDelete: Cascade)
  offer        Offer?      @relation(fields: [offer_id], references: [id])
  product      Product     @relation(fields: [product_id], references: [id], onDelete: Cascade)
  product_unit ProductUnit @relation(fields: [unit_id], references: [id])

  @@unique([cart_id, unit_id])
  @@index([cart_id])
  @@index([offer_id], map: "cart_item_offer_id_fkey")
  @@index([product_id])
  @@index([unit_id])
  @@map("cart_item")
}

model Order {
  id                Int                  @id @default(autoincrement())
  orderNumber       String               @unique
  customerId        String
  totalAmount       Float
  shippingCost      Float                @default(0)
  discount          Float                @default(0)
  taxAmount         Float                @default(0)
  status            orders_status        @default(PENDING)
  paymentMethod     orders_paymentMethod @default(CASH_ON_DELIVERY)
  paymentStatus     orders_paymentStatus @default(PENDING)
  shippingAddress   String               @db.LongText
  notes             String?
  createdAt         DateTime             @default(now())
  updatedAt         DateTime
  paymentId         String?
  userAddressId     Int?
  refundedAmount    Float                @default(0)
  cartId            Int?
  customerAddressId Int?
  PaymentAttempt    PaymentAttempt[]
  assignments       OrderAssignment[]

  orderItems      OrderItem[]
  cart            Cart?            @relation(fields: [cartId], references: [id])
  customerAddress CustomerAddress? @relation(fields: [customerAddressId], references: [id])
  user            User             @relation(fields: [customerId], references: [id])
  refunds         Refund[]

  @@index([cartId], map: "orders_cartId_fkey")
  @@index([customerAddressId], map: "orders_customerAddressId_fkey")
  @@index([customerId], map: "orders_customerId_fkey")
  @@map("orders")
}

model OrderItem {
  id          Int          @id @default(autoincrement())
  orderId     Int
  productId   String
  quantity    Int
  price       Float
  createdAt   DateTime     @default(now())
  offer_id    Int?
  offer       Offer?       @relation(fields: [offer_id], references: [id])
  orders      Order        @relation(fields: [orderId], references: [id], onDelete: Cascade)
  product     Product      @relation(fields: [productId], references: [id])
  refundItems RefundItem[]

  @@index([offer_id], map: "order_items_offer_id_fkey")
  @@index([orderId], map: "order_items_orderId_fkey")
  @@index([productId], map: "order_items_productId_fkey")
  @@map("order_items")
}

